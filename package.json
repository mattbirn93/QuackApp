{
  "name": "quackapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": ".",
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "scripts": {
    "dev": "vite",
    "start": "node dist/server.js",
    "build:frontend": "vite build",
    "build:backend": "tsc -p tsconfig.server.json",
    "build": "npm run build:frontend && npm run build:backend",
    "server": "node dist/server.js",
    "dev:server": "nodemon --watch './**/*.ts' --exec 'node --loader ts-node/esm' server.ts",
    "dev:frontend": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "preview": "PORT=5173 node dist/server.js",
    "heroku-postbuild": "npm run build && npm run postbuild",
    "postbuild": "ls -la dist",
    "dev:development": "concurrently \"npm run dev:frontend\" \"npm run dev:server\"",
    "dev:staging": "vite --mode staging",
    "build:development": "npm run build:frontend && npm run build:backend",
    "build:staging": "vite build --mode staging",
    "preview:development": "concurrently \"vite preview --mode development --port 5173\" \"node dist/server.js\"",
    "preview:staging": "concurrently \"vite preview --mode staging --port 5173\" \"node dist/server.js\""
  },
  "dependencies": {
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@gsap/react": "^2.1.1",
    "@react-typed-hooks/use-key-press": "^1.0.5",
    "@relume_io/relume-ui": "^0.3.0",
    "@splinetool/react-spline": "^4.0.0",
    "@studio-freight/lenis": "^1.0.42",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.13",
    "@tiptap/extension-collaboration": "^2.4.0",
    "@tiptap/extension-collaboration-cursor": "^2.4.0",
    "@tiptap/extension-underline": "^2.4.0",
    "@tiptap/pm": "^2.4.0",
    "@tiptap/react": "^2.4.0",
    "@tiptap/starter-kit": "^2.4.0",
    "@types/socket.io-client": "^3.0.0",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "comlink": "^4.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "express-winston": "^4.2.0",
    "framer-motion": "^11.2.6",
    "gsap": "^3.12.5",
    "html-react-parser": "^5.1.10",
    "http-proxy": "^1.18.1",
    "mongoose": "^8.4.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-animated-cursor": "^2.11.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
